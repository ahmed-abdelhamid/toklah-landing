<div class="section-container contact-container" id="contact">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-12">
        <div class="section-container-spacer">
          <h2 class="text-center">الشكاوى والاقتراحات</h2>
        </div>
        <div class="card-container">
          <div class="card card-shadow col-xs-12 reveal">
            <form action="" class="reveal-content">
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <input type="text" class="form-control ar" id="name" placeholder="الاسم" dir="auto">
                  </div>
                  <div class="form-group">
                    <input type="text" class="form-control ar" id="mobile" placeholder="الجوال" dir="auto">
                  </div>
                  <div class="form-group">
                    <input type="email" class="form-control ar" id="email" placeholder="الإيميل" dir="auto">
                  </div>
                  <div class="form-group">
                    <textarea class="form-control ar" rows="3" placeholder="رسالتك" dir="auto"></textarea>
                  </div>
                  <button type="submit" class="btn btn-primary">ارسال</button>
                </div>
              </div>
            </form>
          </div>
          {{!-- <div class="card-image col-xs-12" style="background-image: url('/assets/images/complaints.png')"></div> --}}
        </div>
      </div>
    </div>
  </div>
</div>

